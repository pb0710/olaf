{
    "compilerOptions": {
        "jsx": "react",
        "target": "ESNext",
        "lib": [
            "ESNext",
            "DOM",
            "DOM.Iterable"
        ],
        "module": "ESNext",
        "moduleResolution": "node",
        "strict": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitThis": false,
        "sourceMap": false,
        "declaration": true,
        "outDir": "./dist",
    }
}